{"ast":null,"code":"import ProductModal from '../components/ProductModal.vue';\nexport default {\n  data() {\n    return {\n      product: [],\n      pagination: {},\n      tempProduct: {},\n      isNew: false\n    };\n  },\n  components: {\n    ProductModal\n  },\n  methods: {\n    openModal(isNew, item) {\n      if (isNew) {\n        this.tempProduct = {};\n      } else {\n        this.tempProduct = {\n          ...item\n        };\n      }\n      this.isNew = isNew;\n      const productComponent = this.$refs.productModal;\n      productComponent.showModal();\n    },\n    getProducts() {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/products`;\n      this.$http.get(api).then(res => {\n        if (res.data.success) {\n          console.log(res);\n          this.product = res.data.products;\n          this.pagination = res.data.pagination;\n        }\n      });\n    },\n    updateProduct(item) {\n      this.tempProduct = item;\n      console.log(this.tempProduct);\n      let api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product`;\n      let httpMethod = 'PUT';\n      const productComponent = this.$refs.productModal;\n      this.$http.post(api, {\n        data: this.tempProduct\n      }).then(res => {\n        console.log(res);\n        productComponent.hideModal();\n        this.getProducts();\n      });\n    }\n  },\n  created() {\n    this.getProducts();\n  }\n};","map":{"version":3,"names":["ProductModal","data","product","pagination","tempProduct","isNew","components","methods","openModal","item","productComponent","$refs","productModal","showModal","getProducts","api","process","env","VUE_APP_API","VUE_APP_PATH","$http","get","then","res","success","console","log","products","updateProduct","httpMethod","post","hideModal","created"],"sources":["/Users/luojidong/vue學習/vue-最終作業/vue-final-project/src/views/ProductsView.vue"],"sourcesContent":["<template>\n    <div class=\"text-end\">\n      <button class=\"btn btn-primary\" type=\"button\"\n      @click=\"openModal(true)\">\n        增加一個商品\n      </button>\n    </div>\n    <table class=\"table mt-4\">\n    <thead>\n        <tr>\n        <th width=\"120\">分類</th>\n        <th>產品名稱</th>\n        <th width=\"120\">原價</th>\n        <th width=\"120\">售價</th>\n        <th width=\"100\">是否啟用</th>\n        <th width=\"200\">編輯</th>\n        </tr>\n    </thead>\n    <tbody>\n        <tr v-for=\"item in product\" :key=\"item.id\">\n        <td>{{item.category}}</td>\n        <td>{{item.title}}</td>\n        <td class=\"text-right\">\n            {{item.origin_price}}\n        </td>\n        <td class=\"text-right\">\n            {{item.price}}\n        </td>\n        <td>\n            <span class=\"text-success\" v-if=\"item.is_enabled\">啟用</span>\n            <span class=\"text-muted\" v-else>未啟用</span>\n        </td>\n        <td>\n            <div class=\"btn-group\">\n            <button class=\"btn btn-outline-primary btn-sm\" @click=\"openModal(false, item)\">編輯</button>\n            <button class=\"btn btn-outline-danger btn-sm\">刪除</button>\n            </div>\n        </td>\n        </tr>\n    </tbody>\n    </table>\n    <ProductModal ref=\"productModal\"\n    :product=\"tempProduct\" @update-product=\"updateProduct\"></ProductModal>\n</template>\n\n<script>\nimport ProductModal from '../components/ProductModal.vue'\n\nexport default {\n  data () {\n    return {\n      product: [],\n      pagination: {},\n      tempProduct: {},\n      isNew: false\n    }\n  },\n  components: {\n    ProductModal\n  },\n  methods: {\n    openModal (isNew, item) {\n      if (isNew) {\n        this.tempProduct = {}\n      } else {\n        this.tempProduct = { ...item }\n      }\n      this.isNew = isNew\n      const productComponent = this.$refs.productModal\n      productComponent.showModal()\n    },\n    getProducts () {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/products`\n      this.$http.get(api).then((res) => {\n        if (res.data.success) {\n          console.log(res)\n          this.product = res.data.products\n          this.pagination = res.data.pagination\n        }\n      })\n    },\n    updateProduct (item) {\n      this.tempProduct = item\n      console.log(this.tempProduct)\n      let api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product`\n      let httpMethod = 'PUT'\n      const productComponent = this.$refs.productModal\n      this.$http.post(api, { data: this.tempProduct }).then((res) => {\n        console.log(res)\n        productComponent.hideModal()\n        this.getProducts()\n      })\n    }\n  },\n  created () {\n    this.getProducts()\n  }\n}\n</script>\n"],"mappings":"AA8CA,OAAOA,YAAW,MAAO,gCAA+B;AAExD,eAAe;EACbC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,CAAC,CAAC;MACdC,WAAW,EAAE,CAAC,CAAC;MACfC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,UAAU,EAAE;IACVN;EACF,CAAC;EACDO,OAAO,EAAE;IACPC,SAAQA,CAAGH,KAAK,EAAEI,IAAI,EAAE;MACtB,IAAIJ,KAAK,EAAE;QACT,IAAI,CAACD,WAAU,GAAI,CAAC;MACtB,OAAO;QACL,IAAI,CAACA,WAAU,GAAI;UAAE,GAAGK;QAAK;MAC/B;MACA,IAAI,CAACJ,KAAI,GAAIA,KAAI;MACjB,MAAMK,gBAAe,GAAI,IAAI,CAACC,KAAK,CAACC,YAAW;MAC/CF,gBAAgB,CAACG,SAAS,CAAC;IAC7B,CAAC;IACDC,WAAUA,CAAA,EAAK;MACb,MAAMC,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAG,CAACC,WAAY,OAAMF,OAAO,CAACC,GAAG,CAACE,YAAa,iBAAe;MACrF,IAAI,CAACC,KAAK,CAACC,GAAG,CAACN,GAAG,CAAC,CAACO,IAAI,CAAEC,GAAG,IAAK;QAChC,IAAIA,GAAG,CAACtB,IAAI,CAACuB,OAAO,EAAE;UACpBC,OAAO,CAACC,GAAG,CAACH,GAAG;UACf,IAAI,CAACrB,OAAM,GAAIqB,GAAG,CAACtB,IAAI,CAAC0B,QAAO;UAC/B,IAAI,CAACxB,UAAS,GAAIoB,GAAG,CAACtB,IAAI,CAACE,UAAS;QACtC;MACF,CAAC;IACH,CAAC;IACDyB,aAAYA,CAAGnB,IAAI,EAAE;MACnB,IAAI,CAACL,WAAU,GAAIK,IAAG;MACtBgB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACtB,WAAW;MAC5B,IAAIW,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAG,CAACC,WAAY,OAAMF,OAAO,CAACC,GAAG,CAACE,YAAa,gBAAc;MAClF,IAAIU,UAAS,GAAI,KAAI;MACrB,MAAMnB,gBAAe,GAAI,IAAI,CAACC,KAAK,CAACC,YAAW;MAC/C,IAAI,CAACQ,KAAK,CAACU,IAAI,CAACf,GAAG,EAAE;QAAEd,IAAI,EAAE,